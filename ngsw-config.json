{
  "index": "/index.html",
  "assetGroups": [{
    "name": "app",
    "installMode": "prefetch",
    "resources": {
      "files": [
        "/favicon.ico",
        "/index.html",
        "/**/*.css",
        "/**/*.js"
      ],
      "urls": [
        "https://fonts.googleapis.com/css?family=Roboto",
        "https://fonts.gstatic.com/**",
        "https://fonts.googleapis.com/icon?family=Material+Icons"
      ]
    }
  }, {
    "name": "assets",
    "installMode": "lazy",
    "updateMode": "prefetch",
    "resources": {
      "files": [
        "/assets/**"
      ]
    }
  }],
  "dataGroups": [{
    "name": "currencies",
    "urls": [
      "https://restcountries.eu/rest/v2/*"
    ],
    "cacheConfig": {
      "maxSize": "10",
      "maxAge": "12h",
      "timeout": "10s",
      "strategy": "freshness"
    }
  }]
}
